{
  "name": "WikiZero Redirector",
  "description": "Redirect Wikipedia to WikiZero",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "wiki.png"
  },
  "icons": {
    "48": "wiki.png"
  },
  "permissions": [
    "webRequest",
    "*://*.wikipedia.org/wiki/*",
    "webRequestBlocking"
  ]
}